{
  "name": "kieren.nz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-analytics": "^5.10.3",
    "vue-awesome": "^3.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.7",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.7",
    "@vue/cli-service": "^3.0.0-beta.7",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.7",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": [
        2,
        "tab"
      ],
      "no-tabs": 0,
      "max-len": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
